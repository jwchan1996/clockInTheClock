{
    "manifest_version": 2,
    "name": "clockInTheClock",
    "version": "1.0.0",
    "description": "艺术风格的打卡时钟",
    "icons": {
        "16": "img/icon.png",
        "48": "img/icon.png",
        "128": "img/icon.png"
    },
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_title": "艺术风格的打卡时钟"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "js/sentence.js",
                "js/script.js"
            ],
            "css": [
                "css/style.css"
            ],
            "run_at": "document_start"
        }
    ],
    "homepage_url": "https://xingorg1.github.io/clockInTheClock/index.html"
}